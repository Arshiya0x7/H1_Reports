  "summaries": [
    {
      "id": 1102,
      "category": "team",
      "content": "Pusher authentication did not take device confirmation into account.\n\nThis would allow an attacker with a valid session but an unconfirmed device to snoop on pusher updates like incoming transactions.\n",
      "updated_at": "2015-12-11T08:15:40.754Z",
      "can_view?": true,
      "can_create?": false,
      "attachments": [],
      "user": {
        "id": 12089,
        "username": "coinbase2",
        "name": "Coinbase",
        "bio": "",
        "cleared": false,
        "verified": false,
        "website": "https://www.coinbase.com",
        "location": "",
        "created_at": "2014-09-16T01:36:14.752Z",
        "url": "https://hackerone.com/coinbase2",
