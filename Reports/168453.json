    {
      "id": 2824,
      "category": "team",
      "content": "When applying for monthly billing, statistics and data regarding account usage that was fetched and subsequently submitted could be tampered with. This data would be reviewed by a human making a credit decision, potentially influencing their decision inappropriately in favor of the applicant.",
      "updated_at": "2016-10-20T20:02:57.042Z",
      "can_view?": true,
      "can_create?": false,
      "attachments": [],
      "user": {
        "id": 11536,
        "username": "alexg-uber",
        "name": "Alex Garbutt",
        "bio": "",
        "cleared": false,
        "verified": false,
        "website": null,
        "location": "",
        "created_at": "2014-09-05T18:11:52.152Z",
        "url": "https://hackerone.com/alexg-uber",
        "hackerone_triager": false,
        "hackerone_employee": false,
        "user_type": "hacker",
        "profile_picture_urls": {
          "small": "/assets/avatars/default-14ffa99f59cd01423c64904352cc130ffcb6a802eadfd11777a54485749e60f2.png",
          "medium": "/assets/avatars/default-14ffa99f59cd01423c64904352cc130ffcb6a802eadfd11777a54485749e60f2.png",
